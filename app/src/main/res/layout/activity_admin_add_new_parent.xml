    <EditText
        android:id="@+id/editTextTextPersonName8"
        android:layout_width="140dp"
        android:layout_height="51dp"
        android:layout_marginTop="220dp"
        android:layout_marginEnd="40dp"
        android:ems="10"
        android:inputType="textPersonName"
