<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdminChildren">

    <SearchView
        android:id="@+id/csearchView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginHorizontal="30dp"
        android:layout_marginTop="5dp"
        android:autofillHints="Enter name"
        android:gravity="center"
        android:iconifiedByDefault="true"
        android:outlineSpotShadowColor="@color/theme"
        android:paddingBottom="10dp"
        android:queryHint="Enter name"
        app:layout_constraintBottom_toBottomOf="@+id/recyclerView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023">

    </SearchView>

    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/childrenListView"
        android:layout_marginTop="60dp"
        android:background="#fff"/>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/btn_cadd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="30dp"
        android:backgroundTint="@color/theme"
        android:onClick="btn_cadd_activity"
        android:src="@drawable/ic_baseline_person_add_24" />


</RelativeLayout>

